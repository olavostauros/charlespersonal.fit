<svg width="800" height="600" viewBox="0 0 800 600" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="trainingBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10B981;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#3B82F6;stop-opacity:0.1" />
    </linearGradient>
  </defs>
  
  <rect width="800" height="600" fill="url(#trainingBg)"/>
  
  <!-- Gym equipment silhouettes -->
  <g transform="translate(100, 150)">
    <!-- Dumbbell -->
    <rect x="50" y="90" width="80" height="15" rx="7" fill="#6B7280" opacity="0.4"/>
    <circle cx="45" cy="97" r="12" fill="#6B7280" opacity="0.4"/>
    <circle cx="135" cy="97" r="12" fill="#6B7280" opacity="0.4"/>
    
    <!-- Person training -->
    <circle cx="200" cy="60" r="25" fill="#4B5563" opacity="0.3"/>
    <rect x="185" y="80" width="30" height="60" rx="15" fill="#4B5563" opacity="0.3"/>
    <rect x="160" y="95" width="20" height="40" rx="10" fill="#4B5563" opacity="0.3"/>
    <rect x="205" y="95" width="20" height="40" rx="10" fill="#4B5563" opacity="0.3"/>
    <rect x="190" y="135" width="10" height="50" rx="5" fill="#4B5563" opacity="0.3"/>
    <rect x="205" y="135" width="10" height="50" rx="5" fill="#4B5563" opacity="0.3"/>
    
    <!-- Barbell -->
    <rect x="300" y="120" width="200" height="8" rx="4" fill="#6B7280" opacity="0.4"/>
    <rect x="290" y="115" width="20" height="18" rx="3" fill="#6B7280" opacity="0.4"/>
    <rect x="510" y="115" width="20" height="18" rx="3" fill="#6B7280" opacity="0.4"/>
  </g>
  
  <!-- Text overlay -->
  <text x="400" y="350" text-anchor="middle" fill="#4B5563" font-family="Inter, sans-serif" font-size="28" font-weight="600">
    Charles em Ação
  </text>
  <text x="400" y="380" text-anchor="middle" fill="#6B7280" font-family="Inter, sans-serif" font-size="18">
    Training Session Placeholder
  </text>
  
  <!-- Motivational elements -->
  <circle cx="120" cy="100" r="4" fill="#F59E0B" opacity="0.7"/>
  <circle cx="680" cy="150" r="5" fill="#10B981" opacity="0.7"/>
  <circle cx="150" cy="450" r="3" fill="#3B82F6" opacity="0.7"/>
  <circle cx="650" cy="500" r="4" fill="#F59E0B" opacity="0.7"/>
  
  <!-- Progress indicators -->
  <rect x="50" y="500" width="100" height="8" rx="4" fill="#E5E7EB"/>
  <rect x="50" y="500" width="75" height="8" rx="4" fill="#10B981"/>
  
  <rect x="200" y="500" width="100" height="8" rx="4" fill="#E5E7EB"/>
  <rect x="200" y="500" width="60" height="8" rx="4" fill="#3B82F6"/>
  
  <rect x="350" y="500" width="100" height="8" rx="4" fill="#E5E7EB"/>
  <rect x="350" y="500" width="90" height="8" rx="4" fill="#F59E0B"/>
</svg>
